{
  "name": "starter-file",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile_sass": "node-sass -w sass/fichier.scss dist/css/style.css",
    "compress_sass": "node-sass --output-style=compressed -w sass/fichier.scss dist/css/style.css",
    "prefix_css": "postcss --watch=true --no-map --use autoprefixer -o dist/css/style.css sass/fichier.scss",
    "concat-css": "concat -o dist/css/style.concat.css dist/css/additional.css dist/css/style.css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.7.5",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "postcss-cli": "^7.1.0"
  }
}

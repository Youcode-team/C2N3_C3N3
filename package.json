  "scripts": {
    "compil": "node-sass Starter-file/sass -o Starter-file/dist/css",
    "watch": "node-sass -w Starter-file/sass -o Starter-file/dist/css",
    "compress": "node-sass --output-style compressed Starter-file/sass -o Starter-file/dist/css",
    "concat": "concat -o Starter-file/dist/css/style.css Starter-file/dist/css/fichier.css Starter-file/dist/css/fichier1.css Starter-file/dist/css/fichier2.css Starter-file/dist/css/fichier3.css",
    "prefix": "postcss --use autoprefixer -b 'last 5 versions' Starter-file/dist/css/fichier.css -o Starter-file/dist/css/style.prefix.css"
  },

{
  "name": "covid20",
  "version": "1.0.0",
  "description": "scripting",
  "main": "index.js",
  "scripts": {
    "watch-sass": "node-sass sass/main.scss css/style.css --watch",
    "compile-sass": "node-sass sass/main.scss css/style.comp.css",
    "concat-css": "concat -o css/style.concat.css css/style.css css/style.comp.css",
    "prefix-css": "postcss --watch=true --no-map --use autoprefixer -o css/style.prefix.css css/style.concat.css",
    "compress-css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build-css": "npm-run-all compile-sass concat-css prefix-css compress-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlackDiamond-UX/C2N3_C3N3.git"
  },
  "author": "Ayoub Rouiha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BlackDiamond-UX/C2N3_C3N3/issues"
  },
  "homepage": "https://github.com/BlackDiamond-UX/C2N3_C3N3#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
}
